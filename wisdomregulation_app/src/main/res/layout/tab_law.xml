<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.wisdomregulation.allactivity.single.Activity_Welcome" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="153"
        android:background="#2378BB"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="44"
            android:orientation="horizontal" >
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="62"
            android:clickable="true"
            android:onClick="back"
            android:orientation="horizontal" >

            <com.wisdomregulation.frame.AutoClickImageView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="92"
                android:gravity="center"
                android:scaleType="centerCrop"
                android:src="@drawable/plan_02" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="20" />

            <TextView
                android:id="@+id/newtitle"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="740"
                android:gravity="left|center_vertical"
                android:text="执法检查"
                android:textColor="@color/white" />

            <!-- <com.wisdomregulation.frame.AutoClickImageView -->
            <!-- android:layout_width="0dp" -->
            <!-- android:layout_height="match_parent" -->
            <!-- android:layout_weight="180" -->
            <!-- android:clickable="true" -->
            <!-- android:onClick="showAgainAdvice" -->
            <!-- android:scaleType="fitStart" -->
            <!-- android:src="@drawable/again_03" /> -->

            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="220" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="44"
            android:orientation="horizontal" >
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="3"
            android:background="#B2C0CB"
            android:orientation="horizontal" >
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="254"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="236"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="36" >
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="164"
                android:orientation="horizontal" >

                <com.wisdomregulation.frame.PassLinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="114" >

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="33" >
                    </LinearLayout>

                    <com.wisdomregulation.frame.AutoClickImageView
                        android:id="@+id/scrollLeft"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="51"
                        android:clickable="true"
                        android:onClick="scrollLeft"
                        android:scaleType="fitCenter"
                        android:src="@drawable/dangeroush_04" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="30" >
                    </LinearLayout>
                </com.wisdomregulation.frame.PassLinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="852" >

                    <HorizontalScrollView
                        android:id="@+id/levellist"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scrollbars="none" >

                        <LinearLayout
                            android:id="@+id/levelcontent"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal" >
                        </LinearLayout>
                    </HorizontalScrollView>
                </LinearLayout>

                <com.wisdomregulation.frame.PassLinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="114" >

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="30" >
                    </LinearLayout>

                    <com.wisdomregulation.frame.AutoClickImageView
                        android:id="@+id/scrollRight"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="51"
                        android:clickable="true"
                        android:onClick="scrollRight"
                        android:scaleType="fitCenter"
                        android:src="@drawable/dangeroush_03" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="33" >
                    </LinearLayout>
                </com.wisdomregulation.frame.PassLinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="36" >
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#DCDCDC" >
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="17"
            android:background="#EEEEEE" >
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="148"
        android:background="#EEEEEE"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="30"
            android:background="#EEEEEE"
            android:orientation="horizontal" >
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="70"
            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1080"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="20" >
                </LinearLayout>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="100"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:id="@+id/history1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:visibility="gone" >

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="40" />

                        <com.wisdomregulation.frame.AutoClickImageView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="220"
                            android:clickable="true"
                            android:onClick="addBook"
                            android:scaleType="fitCenter"
                            android:src="@drawable/book_03" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="40" />

                        <com.wisdomregulation.frame.AutoClickImageView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="220"
                            android:clickable="true"
                            android:onClick="delets"
                            android:scaleType="fitCenter"
                            android:src="@drawable/book_05" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="40" />

                        <com.wisdomregulation.frame.AutoClickImageView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="220"
                            android:clickable="true"
                            android:onClick="prints"
                            android:scaleType="fitCenter"
                            android:src="@drawable/book_07" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="40" />

                        <com.wisdomregulation.frame.AutoClickImageView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="220"
                            android:clickable="true"
                            android:onClick="exports"
                            android:scaleType="fitCenter"
                            android:src="@drawable/book_09" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="40" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/history2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:visibility="gone" >

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="40" />

                        <com.wisdomregulation.frame.AutoClickImageView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="220"
                            android:clickable="true"
                            android:onClick="prints"
                            android:scaleType="fitCenter"
                            android:src="@drawable/book_07" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="40" />

                        <com.wisdomregulation.frame.AutoClickImageView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="220"
                            android:clickable="true"
                            android:onClick="exports"
                            android:scaleType="fitCenter"
                            android:src="@drawable/book_09" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="40" />
                    </LinearLayout>
                </FrameLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="20" >
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="30"
            android:background="#EEEEEE"
            android:orientation="horizontal" >
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:background="#BDBDBD"
            android:orientation="horizontal" >
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1290"
        android:orientation="vertical" >

        <ListView
            android:id="@+id/bookList"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="@null"
            android:scrollbars="none" >
        </ListView>
    </LinearLayout>

</LinearLayout>